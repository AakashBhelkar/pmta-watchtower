# Frontend Environment Configuration Example
# Copy this file to .env and fill in the values

# PMTA Watchtower Frontend Configuration

# App
VITE_BASE_PATH=
# API is proxied through Vite - see vite.config.js
VITE_API_URL=http://localhost:4000

# Authentication (optional - skip for development)
VITE_AUTH_SKIP=true

# Mapbox (optional - for future map features)
VITE_MAPBOX_API_KEY=

# Supabase (optional - if using cloud database)
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=
